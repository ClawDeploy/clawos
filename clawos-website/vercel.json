{
  "version": 2,
  "name": "clawos-website",
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(logo.jpg|favicon.ico|.*.png|.*.svg|.*.css|.*.js)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
