{
  "version": 2,
  "installCommand": "cd apps/web && pnpm install",
  "buildCommand": "cd apps/web && pnpm run build",
  "outputDirectory": "apps/web/.next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://clawos-api-production.up.railway.app/api/$1"
    }
  ]
}