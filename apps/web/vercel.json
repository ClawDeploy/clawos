{
  "version": 2,
  "buildCommand": "cd ../.. && pnpm install && pnpm run build --filter=web",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "pnpm install",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.clawos.xyz/api/:path*"
    }
  ]
}
